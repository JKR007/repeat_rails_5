<%= link_to  "<< Back to List", admin_users_path, :class => 'back-link' %>
<div class="subject delete">
  <h2>Delete Admin</h2>
  <%= form_for @admin_user, :method => 'delete' do |f| %>
    <p>Are you sure you want to delete this subject permanently</p>
    <p class="reference-object"><%= f.object.full_name %></p>
    <div class="form-buttons">
      <%= f.submit "Delete Subject"%>
    </div>
  <% end %>
</div>